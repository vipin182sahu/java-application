package Reflection;
import java.awt.*;
import java.lang.reflect.*;

class privateObject{
	private String privateString = null;
	public privateObject(String privateString){
		this.privateString = privateString;
	}
	
}
public class Temp {
	public static void mian(String[] ss) throws SecurityException, NoSuchFieldException{
		privateObject po = new privateObject("the private value");
		Class c = privateObject.class.getClass();
		Field privateStringField = c.getDeclaredField("privateString");
		privateStringField.setAccessible(true);
	}
}
